<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context="com.promote.ebingo.InformationActivity.InterpriseInfoActivity">

    <include layout="@layout/common_head_layout"></include>

    <com.jch.lib.view.PagerSlidingTabStrip
        android:layout_width="match_parent"
        android:id="@+id/interprise_info_tab"
        app:indicatorColor="@color/orange_my"
        app:indicatorHeight="2dp"
        app:tabtextSize="@dimen/font_18"
        app:tabtextAllCaps="false"
        app:underlineColor="@color/info_name_gray"
        app:tabtextColor="@color/info_name_gray"
        app:underlineHeight="1dp"
        android:layout_height="@dimen/sliding_tab_height"/>


    <android.support.v4.view.ViewPager
        android:id="@+id/interprise_info_vp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </android.support.v4.view.ViewPager>


</LinearLayout>
